
@model List<TeaShopApi.WebUI.Dtos.DrinkDtos.ResultDrinkDto>
<div class="services_section layout_padding">
	<div class="container">
		<h1 class="services_taital">İçecek Menümüz</h1>
		<p class="services_text">menümüzdeki içecekler ve fiyatlar listesi.. </p>
		<div class="services_section_2 layout_padding">
			<div class="row">
				@foreach (var item in Model)
				{
					<div class="col-md-6">
						<div class="box_main ">
							<div class="left_main">
								<div class="cup_img_1"><img src="@item.drinkImageUrl" style="height:150px; width:200px;"></div>
							</div>
							<div class="middle_main">
								<div class="border_10 "></div>
							</div>
							<div class="right_main">
								<h6 class="milk_text">@item.drinkName</h6>
								<h1 class="price_text">@item.drinkPrice ₺</h1>
							</div>
						</div>
					</div>
				}
			</div>
		</div>
		<div class="see_bt"><a href="#">Tüm İçecekleri Gör</a></div>
	</div>
</div>